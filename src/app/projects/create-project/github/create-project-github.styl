.github-create-project .repo-icon
  color #666

.github-create-project .repo-name
  font-weight bold

.github-create-project .repo-private
  color #a1882b

.github-create-project span
  margin-left 5px

.github-create-project .repo-description
  color #777
  line-height 21px

.github-create-project
  cursor:pointer

.github-create-project-active md-item-content
  background-color active-color !important
  color #FFFFFF

.github-create-project md-item:hover md-item-content
  background-color #ADD8E6 !important
  color #FFFFFF

.create-project-github-panel
  margin 15px

.github-create-project md-item:nth-child(2n) md-item-content
  background-color #FCFCFC


.github-create-project md-item:nth-child(2n+1) md-item-content
  background-color #FFFFFF

.github-create-project .icons
  margin-right 20px

.github-create-project-select
  display block
  width 100%
  height 34px
  padding 6px 12px
  font-size 14px
  line-height 1.42857143
  background-color #fafafa
  background-image none
  border 1px solid #e5e5e5
  border-radius 0
  -webkit-box-shadow inset 0 1px 1px rgba(0,0,0,.075)
  -webkit-transition border-color ease-in-out .15s,box-shadow ease-in-out .15s
  transition border-color ease-in-out .15s,box-shadow ease-in-out .15s
